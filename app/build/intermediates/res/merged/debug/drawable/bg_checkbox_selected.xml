<?xml version="1.0" encoding="utf-8"?>
<!-- 顶部带圆角 白色背景 灰色边框 无下边框 长方体 -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape>
            <solid android:color="@color/blue" />

            <size
                android:height="12dp"
                android:width="12dp" />

            <stroke
                android:width="1dp"
                android:color="@color/white" />
        </shape>
    </item>

</layer-list>